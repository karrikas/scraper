{
    "name": "karrikas/scraper",
    "type": "library",
    "description": "web scraper",
    "keywords": ["framework", "scraper"],
    "homepage": "https://github.com/karrikas/scraper",
    "license": "MIT",
    "autoload": {
        "psr-4": {"Scraper\\": "src/"}
    },
    "authors": [
        {
            "name": "karrikas",
            "email": "karrikas@karrikas.com",
            "homepage": "http://karrikas.com",
            "role": "Developer"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "symfony/console": "^3.3",
        "fabpot/goutte": "^3.2",
        "tedivm/stash": "^0.14.2",
        "symfony/dom-crawler": "^3.3",
        "symfony/browser-kit": "^3.3",
        "monolog/monolog": "^1.23"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-0": {
            "Monolog": "src"
